{"dokumente":{"@builddate":"20120625155024","@doknr":"BJNR083500003","norm":[{"@builddate":"20120625155024","@doknr":"BJNR083500003","metadaten":{"jurabk":"WarnowMautHV","amtabk":"WarnowMautHV","ausfertigung-datum":{"@manuell":"ja","#text":"2003-06-10"},"fundstelle":{"@typ":"amtlich","periodikum":"BGBl I","zitstelle":"2003, 835"},"kurzue":"Warnow-Tunnel-Mauthöheverordnung","langue":"Verordnung über die Höhe der Maut für die Benutzung des Warnowtunnels"},"textdaten":{"text":{"@format":"XML","Content":{"P":null}},"fussnoten":{"Content":{"P":{"BR":null,"pre":{"@xml:space":"preserve","BR":[null,null],"#text":"(+++ Textnachweis ab: 12. 6.2003 +++)"}}}}}},{"@builddate":"20120625155024","@doknr":"BJNR083500003BJNE000100000","metadaten":{"jurabk":"WarnowMautHV","enbez":"Eingangsformel"},"textdaten":{"text":{"@format":"XML","Content":{"P":"Auf Grund des § 3a Abs. 2 Satz 1 in Verbindung mit § 3 Abs. 2 bis 5 des Fernstraßenbauprivatfinanzierungsgesetzes in der Fassung der Bekanntmachung vom 20. Januar 2003 (BGBl. I S. 98) verordnet das Bundesministerium für Verkehr, Bau- und Wohnungswesen nach Anhörung des Wirtschaftsministeriums des Landes Mecklenburg-Vorpommern:"}}}},{"@builddate":"20120625155024","@doknr":"BJNR083500003BJNE000200000","metadaten":{"jurabk":"WarnowMautHV","enbez":"§ 1","titel":{"@format":"parat","#text":"Mauthöhe"}},"textdaten":{"text":{"@format":"XML","Content":{"P":"Für jede einfache Benutzung des Abschnittes der Bundesstraße B 103n zwischen Kilometer 1 + 040 und Kilometer 3 + 160 (Warnow-Tunnel im Stadtgebiet der Hansestadt Rostock) wird von der Warnowquerung GmbH & Co. KG, Rostock, die vom Wirtschaftsministerium des Landes Mecklenburg-Vorpommern durch die Verordnung zur Beleihung mit dem Recht zur Erhebung von Mautgebühren für die Warnowquerung vom 1. April 2003 (GVBl. M-V S. 280) beliehen worden ist, eine Maut nach dem Mautverzeichnis in der Anlage erhoben."}}}},{"@builddate":"20120625155024","@doknr":"BJNR083500003BJNE000300000","metadaten":{"jurabk":"WarnowMautHV","enbez":"§ 2","titel":{"@format":"parat","#text":"Beginn der Mauterhebung"}},"textdaten":{"text":{"@format":"XML","Content":{"P":"Die Mauterhebung beginnt mit dem Tag der Freigabe der in § 1 genannten Strecke für den öffentlichen Verkehr. Der in Satz 1 genannte Tag wird vom Bundesministerium für Verkehr, Bau- und Wohnungswesen im Bundesanzeiger bekannt gegeben."}}}},{"@builddate":"20120625155024","@doknr":"BJNR083500003BJNE000400000","metadaten":{"jurabk":"WarnowMautHV","enbez":"§ 3","titel":{"@format":"parat","#text":"Inkrafttreten"}},"textdaten":{"text":{"@format":"XML","Content":{"P":"Diese Verordnung tritt am Tage nach der Verkündung in Kraft."}}}},{"@builddate":"20120625155024","@doknr":"BJNR083500003BJNE000500000","metadaten":{"jurabk":"WarnowMautHV","enbez":"Anlage","titel":{"@format":"XML","BR":null,"#text":"(zu § 1)Mautverzeichnis"}},"textdaten":{"text":{"@format":"XML","Content":{"P":{"ABWFORMAT":{"@typ":"A"},"kommentar":{"@typ":"Fundstelle","#text":"Fundstelle des Originaltextes: BGBl. I 2003, 836"},"BR":null,"table":[{"@frame":"all","@tocentry":"%yes;","tgroup":{"@align":"left","@char":"","@charoff":"50","@cols":"5","@colsep":"1","@rowsep":"1","colspec":[{"@colname":"col1","@colnum":"1","@colwidth":"15*"},{"@colname":"col2","@colnum":"2","@colwidth":"20*"},{"@colname":"col3","@colnum":"3","@colwidth":"15*"},{"@colname":"col4","@colnum":"4","@colwidth":"20*"},{"@colname":"col5","@colnum":"5","@colwidth":"30*"}],"thead":{"@valign":"bottom","row":[{"entry":{"@VJ":"1","@align":"center","@colsep":"0","@nameend":"col5","@namest":"col1","@valign":"top","#text":"Mauttabelle einschließlich der gesetzlichen Umsatzsteuer von 16%"}},{"entry":[{"@VJ":"1","@align":"center","@colsep":"0","@valign":"top","#text":"Kategorie"},{"@VJ":"1","@align":"center","@colsep":"0","@valign":"top","#text":"Gesamthöhe"},{"@VJ":"1","@align":"center","@colsep":"0","@valign":"top","#text":"Anzahl der Achsen"},{"@VJ":"1","@align":"center","@colsep":"0","@valign":"top","#text":