{"dokumente":{"@builddate":"20231128213511","@doknr":"BJNR000470983","norm":[{"@builddate":"20231128213511","@doknr":"BJNR000470983","metadaten":{"jurabk":"WZG§4BELuaBek","ausfertigung-datum":{"@manuell":"ja","#text":"1983-01-19"},"fundstelle":{"@typ":"amtlich","periodikum":"BGBl I","zitstelle":"1983, 47 (433)"},"langue":"Bekanntmachung zu § 4 des Warenzeichengesetzes","standangabe":{"@checked":"ja","standtyp":"Stand","standkommentar":"Geändert durch Bek. vom 14.1.1992 I 224"}},"textdaten":{"fussnoten":{"Content":{"P":"(+++ Textnachweis ab: 2.2.1983 +++)"}}}},{"@builddate":"20231128213511","@doknr":"BJNR000470983BJNE000100314","metadaten":{"jurabk":"WZG§4BELuaBek","enbez":"I."},"textdaten":{"text":{"@format":"XML","Content":{"P":{"BR":[null,null,null,null,null,null,null],"#text":"Auf Grund des § 4 Abs. 2 Nr. 3 des Warenzeichengesetzes in der Fassung der Bekanntmachung vom 2. Januar 1968 (BGBl. I S. 1, 29) werden amtliche Prüf- und Gewährzeichen bekanntgemacht, die - im Königreich Belgien (Anlage 1), - in der Tschechoslowakischen Sozialistischen Republik (Anlage 2), - in der Föderativen Republik Brasilien (Anlage 3), - in der Republik Kuba (Anlage 4), - in der Republik Korea (Anlage 5) und - in der Portugiesischen Republik (Anlage 6) eingeführt sind."}}}}},{"@builddate":"20231128213511","@doknr":"BJNR000470983BJNE000200314","metadaten":{"jurabk":"WZG§4BELuaBek","enbez":"II."},"textdaten":{"text":{"@format":"XML","Content":{"P":{"BR":[null,null,null,null],"#text":"Auf Grund des § 4 Abs. 2 Nr. 3a desselben Gesetzes wird bekanntgemacht, daß die Bezeichnungen, Abkürzungen und Kennzeichen - des Benelux-Markenamts (Anlage 7), - des Benelux-Amts für Muster und Modelle (Anlage 8) und - der Internationalen Seeschiffahrts-Organisation (Anlage 9) von der Eintragung als Warenzeichen ausgeschlossen sind."}}}}},{"@builddate":"20231128213511","@doknr":"BJNR000470983BJNE000300314","metadaten":{"jurabk":"WZG§4BELuaBek","enbez":"III."},"textdaten":{"text":{"@format":"XML","Content":{"P":"Diese Bekanntmachung ergeht im Anschluß an die Bekanntmachung vom 7. September 1981 (BGBl. I S. 940)."}}}},{"@builddate":"20231128213511","@doknr":"BJNR000470983BJNE000400314","metadaten":{"jurabk":"WZG§4BELuaBek","enbez":"Schlußformel"},"textdaten":{"text":{"@format":"XML","Content":{"P":{"SP":"Der Bundesminister der Justiz"}}}}},{"@builddate":"20231128213511","@doknr":"BJNR000470983BJNE000500314","metadaten":{"jurabk":"WZG§4BELuaBek","enbez":"Anlage 1"},"textdaten":{"text":{"@format":"XML","Content":{"P":{"noindex":[{"kommentar":{"@typ":"Hinweis","#text":"(Inhalt: Nicht darstellbares Muster,"}},{"kommentar":{"@typ":"Fundstelle","#text":"Fundstelle: BGBl. I 1983, 48)"}}],"BR":null}}}}},{"@builddate":"20231128213511","@doknr":"BJNR000470983BJNE000601308","metadaten":{"jurabk":"WZG§4BELuaBek","enbez":"Anlage 2"},"textdaten":{"text":{"@format":"XML","Content":{"P":"(weggefallen)"}},"fussnoten":null}},{"@builddate":"20231128213511","@doknr":"BJNR000470983BJNE000700314","metadaten":{"jurabk":"WZG§4BELuaBek","enbez":"Anlage 3"},"textdaten":{"text":{"@format":"XML","Content":{"P":{"noindex":[{"kommentar":{"@typ":"Hinweis","#text":"(Inhalt: Nicht darstellbares Muster,"}},{"kommentar":{"@typ":"Fundstelle","#text":"Fundstelle: BGBl. I 1983, 49)"}}],"BR":null}}}}},{"@builddate":"20231128213511","@doknr":"BJNR000470983BJNE000800314","metadaten":{"jurabk":"WZG§4BELuaBek","enbez":"Anlage 4"},"textdaten":{"text":{"@format":"XML","Content":{"P":{"noindex":[{"kommentar":{"@typ":"Hinweis","#text":"(Inhalt: Nicht darstellbares Muster,"}},{"kommentar":{"@typ":"Fundstelle","#text":"Fundstelle: BGBl. I 1983, 50)"}}],"BR":null}}}}},{"@builddate":"20231128213511","@doknr":"BJNR000470983BJNE000900314","metadaten":{"jurabk":"WZG§4BELuaBek","enbez":"Anlage 5"},"textdaten":{"text":{"@format":"XML","Content":{"P":{"noindex":[{"kommentar":{"@typ":"Hinweis","#text":"(Inhalt: Nicht darstellbares Muster,"}},{"kommentar":{"@typ":"Fundstelle","#text":"Fundstelle: BGBl. I 1983, 50)"}}],"BR":null}