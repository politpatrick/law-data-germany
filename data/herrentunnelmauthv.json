{"dokumente":{"@builddate":"20130405132117","@doknr":"BJNR210800005","norm":[{"@builddate":"20130405132117","@doknr":"BJNR210800005","metadaten":{"jurabk":"HerrentunnelMautHV","amtabk":"HerrentunnelMautHV","ausfertigung-datum":{"@manuell":"ja","#text":"2005-07-06"},"fundstelle":{"@typ":"amtlich","periodikum":"BGBl I","zitstelle":"2005, 2108"},"kurzue":"Herrentunnel-Mauthöheverordnung","langue":"Verordnung über die Höhe der Maut für die Benutzung des Herrentunnels"},"textdaten":{"text":{"@format":"XML","Content":{"P":null}},"fussnoten":{"Content":{"P":{"BR":null,"pre":{"@xml:space":"preserve","BR":[null,null],"#text":"(+++ Textnachweis ab: 14. 7.2005 +++)"}}}}}},{"@builddate":"20130405132117","@doknr":"BJNR210800005BJNE000100000","metadaten":{"jurabk":"HerrentunnelMautHV","enbez":"Eingangsformel"},"textdaten":{"text":{"@format":"XML","Content":{"P":"Auf Grund des § 3a Abs. 2 Satz 1 in Verbindung mit § 3 Abs. 2 bis 5 des Fernstraßenbauprivatfinanzierungsgesetzes in der Fassung der Bekanntmachung vom 20. Januar 2003 (BGBl. I S. 98) verordnet das Bundesministerium für Verkehr, Bau- und Wohnungswesen nach Anhörung des Ministeriums für Wissenschaft, Wirtschaft und Verkehr des Landes Schleswig-Holstein:"}}}},{"@builddate":"20130405132117","@doknr":"BJNR210800005BJNE000200000","metadaten":{"jurabk":"HerrentunnelMautHV","enbez":"§ 1","titel":{"@format":"parat","#text":"Mauthöhe"}},"textdaten":{"text":{"@format":"XML","Content":{"P":"Für jede einfache Benutzung des Abschnitts der Bundesstraße 104 zwischen Straßenkilometer 6 + 034,11 westlich der Trave und Straßenkilometer 8 + 051,08 östlich der Trave (Herrentunnel im Stadtgebiet der Hansestadt Lübeck) wird von der Herrentunnel Lübeck GmbH & Co. KG, Lübeck, die vom Ministerium für Wissenschaft, Wirtschaft und Verkehr des Landes Schleswig-Holstein durch § 1 der Landesverordnung über die Beleihung zur Erhebung von Mautgebühren für den Herrentunnel in Lübeck und zur Übertragung der Ermächtigung zur Beleihung vom 22. August 2003 (GVOBl. Schl-H. S. 437), die zuletzt durch die Landesverordnung vom 27. Juni 2005 (GVOBl. Schl-H. S. 265) geändert worden ist, beliehen worden ist, eine Maut nach dem Mautverzeichnis in der Anlage erhoben."}}}},{"@builddate":"20130405132117","@doknr":"BJNR210800005BJNE000300000","metadaten":{"jurabk":"HerrentunnelMautHV","enbez":"§ 2","titel":{"@format":"parat","#text":"Beginn der Mauterhebung"}},"textdaten":{"text":{"@format":"XML","Content":{"P":"Die Mauterhebung beginnt mit dem Tag der Freigabe der in § 1 genannten Strecke für den öffentlichen Verkehr. Der in Satz 1 genannte Tag wird vom Bundesministerium für Verkehr, Bau- und Wohnungswesen im Bundesanzeiger bekannt gegeben."}}}},{"@builddate":"20130405132117","@doknr":"BJNR210800005BJNE000400000","metadaten":{"jurabk":"HerrentunnelMautHV","enbez":"§ 3","titel":{"@format":"parat","#text":"Inkrafttreten"}},"textdaten":{"text":{"@format":"XML","Content":{"P":"Diese Verordnung tritt am Tage nach der Verkündung in Kraft."}}}},{"@builddate":"20130405132117","@doknr":"BJNR210800005BJNE000500000","metadaten":{"jurabk":"HerrentunnelMautHV","enbez":"Anlage","titel":{"@format":"XML","BR":null,"#text":"(zu § 1)\n\nMautverzeichnis"}},"textdaten":{"text":{"@format":"XML","Content":{"P":{"table":{"@frame":"bottom","@tocentry":"%yes;","tgroup":{"@align":"left","@char":"","@charoff":"50","@cols":"6","@colsep":"1","@rowsep":"1","colspec":[{"@colname":"col1","@colnum":"1","@colwidth":"20*"},{"@colname":"col2","@colnum":"2","@colwidth":"20*"},{"@colname":"col3","@colnum":"3","@colwidth":"15*"},{"@colname":"col4","@colnum":"4","@colwidth":"20*"},{"@colname":"col5","@colnum":"5","@colwidth":"15*"},{"@colname":"col6","@colnum":"6","@colwidth":"20*"}],"tbody":{"@valign":"top","row":[{"entry":{"@VJ":"1","@align":"left","@colsep":"0","@nameend":"col6","@namest":"col1","@valign":"top","kommentar":{"@typ":"Fundstelle","#text":"(Fundstelle des Originaltextes: BGBl. I 2005, 2109)"}}},{"entry":[{"@VJ":"1","@align":"left","@colsep":"0","@morero